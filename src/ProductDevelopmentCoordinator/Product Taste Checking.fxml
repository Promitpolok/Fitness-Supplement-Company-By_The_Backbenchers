<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductDevelopmentCoordinator.ProductTasteCheckingController">
   <children>
      <Button layoutX="31.0" layoutY="35.0" mnemonicParsing="false" onAction="#BackButtonOnMouseClick" prefHeight="36.0" prefWidth="92.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Label layoutX="147.0" layoutY="85.0" prefHeight="51.0" prefWidth="342.0" text="Product Taste Checking">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ComboBox fx:id="ProductNameComboBox" layoutX="48.0" layoutY="153.0" prefWidth="150.0" promptText="Product Name" />
      <RadioButton fx:id="okRadioButton" layoutX="257.0" layoutY="158.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="60.0" style="-fx-background-color: green;" text="OK" textFill="#f8f8f8">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="notOkRadioButton" layoutX="386.0" layoutY="158.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="92.0" style="-fx-background-color: Red;" text="Not OK" textFill="#f2efef">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </RadioButton>
      <TextArea fx:id="infoTextArea" editable="false" layoutX="58.0" layoutY="253.0" prefHeight="133.0" prefWidth="484.0" text="Info:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></TextArea>
   </children>
</AnchorPane>
