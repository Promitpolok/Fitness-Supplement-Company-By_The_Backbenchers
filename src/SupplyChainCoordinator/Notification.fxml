<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SupplyChainCoordinator.NotificationController">
   <children>
      <Button layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#BackButtonOnMouseClick" prefHeight="31.0" prefWidth="65.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="76.0" mnemonicParsing="false" onAction="#ViewNotificationButtonOnMouseClick" prefHeight="31.0" prefWidth="150.0" text="View Notifications">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="226.0" layoutY="137.0" mnemonicParsing="false" onAction="#SendMessageButtonOnMouseClick" prefHeight="31.0" prefWidth="124.0" text="Send Message">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="enterMessageTextField" layoutX="382.0" layoutY="76.0" promptText="Enter Your Message">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <ComboBox fx:id="selectNameComboBox" layoutX="213.0" layoutY="76.0" prefWidth="150.0" promptText="Select Name" />
      <TextArea fx:id="notificationTextArea" editable="false" layoutX="14.0" layoutY="186.0" prefHeight="200.0" prefWidth="574.0" text="Notifications:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextArea>
   </children>
</AnchorPane>
