<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductDevelopmentCoordinator.NotificationController">
   <children>
      <Button layoutX="16.0" layoutY="29.0" mnemonicParsing="false" onAction="#BackButtonOnMouseClick" prefHeight="31.0" prefWidth="66.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextArea fx:id="notificationTextArea" editable="false" layoutX="16.0" layoutY="186.0" prefHeight="200.0" prefWidth="573.0" text="Notifications:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextArea>
      <Button layoutX="16.0" layoutY="90.0" mnemonicParsing="false" onAction="#ViewNotificationButtonOnMouseClick" prefHeight="31.0" prefWidth="149.0" text="View Notificatons">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="enterYourMessageTextField" layoutX="16.0" layoutY="139.0" promptText="Enter Your Message">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <Button layoutX="226.0" layoutY="90.0" mnemonicParsing="false" onAction="#SendMessageButtonOnMouseClick" prefHeight="31.0" prefWidth="132.0" text="Send Message">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <ComboBox fx:id="selectNameComboBox" layoutX="413.0" layoutY="90.0" prefWidth="150.0" promptText="Select Name" />
   </children>
</AnchorPane>
